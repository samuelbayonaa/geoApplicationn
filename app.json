{
  "expo": {
    "name": "geo",
    "slug": "geo_proyect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/geo.jpg",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/geo.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "geo",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "package": "com.sena.geo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/geo.jpg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.sena.geo"
    },
    "web": {
      "favicon": "./assets/geo.jpg"
    },
    "plugins": ["expo-router", "expo-font"]
  }
}
